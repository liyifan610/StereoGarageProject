<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gctw.stereogarage.mapper.database.OperationMapper">  
	<!--
		DB Columns:
			OperationId
			CreateTimestamp
			LotId
			UserId
			OperationType
  	-->
  	
  	<insert id="insertOneOperation" parameterType="OperationEntity">
  		insert into operationlog
  		(CreateTimestamp, LotId, UserId, OperationType)
  		values
  		(#{createTimestamp},#{lotId},#{userId},#{operationType});
  	</insert>
</mapper>  